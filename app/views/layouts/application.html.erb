<!DOCTYPE html>
<html>
  <head>
    <title>PhanCamp</title>
    <%= favicon_link_tag asset_path('pc_logo.png') %>
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;1,100;1,300;1,400&display=swap" rel="stylesheet">    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
      window.logo = "<%= image_url('logo.png') %>";
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
