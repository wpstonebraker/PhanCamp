<% if logged_in? %>
    <script type="text/javascript">
        window.currentUser = <%= render("api/users/user.json.jbuilder", user: current_user).html_safe %>
    </script>
<% end %>
<script type="text/javascript">
    window.phishAPIKey = "<%= Rails.application.credentials.phish[:api_key] %>"
</script>
<main id="root"></main>
